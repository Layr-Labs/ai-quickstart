{
  "name": "eigenai-quickstart",
  "version": "1.0.0",
  "description": "A ready-to-use scaffold for building verifiable AI agents using Layr Labs' AgentKit adapters",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "agentkit",
    "opacity",
    "eigenda",
    "witnesschain",
    "verifiable-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@layr-labs/agentkit-opacity": "latest",
    "@layr-labs/agentkit-eigenda": "latest",
    "@layr-labs/agentkit-witnesschain": "latest",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
} 